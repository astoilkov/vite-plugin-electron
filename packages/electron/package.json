{
  "name": "vite-plugin-electron",
  "version": "0.9.2",
  "description": "Integrate Vite and Electron",
  "main": "./index.mjs",
  "types": "types",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electron-vite/vite-plugin-electron.git",
    "directory": "packages/electron"
  },
  "author": "草鞋没号 <308487730@qq.com>",
  "license": "MIT",
  "scripts": {
    "dev": "node build.js --watch",
    "dev:es": "rollup -c -f es --watch",
    "dev:cjs": "rollup -c -f cjs --watch",
    "build": "node build.js",
    "build:es": "rollup -c -f es",
    "build:cjs": "rollup -c -f cjs",
    "types": "tsc --emitDeclarationOnly",
    "prepublishOnly": "rm -rf types && node ../../scripts/check-command.js && npm run build && npm run types"
  },
  "dependencies": {
    "vite-plugin-electron-renderer": "workspace:*"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.4.0",
    "rollup": "^2.79.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.9"
  },
  "files": [
    "src",
    "index.mjs",
    "index.cjs",
    "electron-env.d.ts"
  ],
  "keywords": [
    "vite",
    "plugin",
    "electron",
    "renderer"
  ]
}
